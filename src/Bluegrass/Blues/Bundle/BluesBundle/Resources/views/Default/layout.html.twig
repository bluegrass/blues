{% extends '::layout.html.twig' %}

{% block javascripts %}
	{{ parent() }}

	{% render "BluesgrassBluesBluesBundle:ViewState:headers"  %}
{% endblock %}

{% block stylesheets %}
	{{ parent() }}

        {% stylesheets '@BluegrassBluesBluesBundle/Resources/public/css/ViewState/viewState.css' %}   	            
        <link href="{{ asset_url }}" media="screen" rel="stylesheet" type="text/css" />
        {% endstylesheets %}

        {% stylesheets '@BluegrassBluesBreadcrumbBundle/Resources/public/css/breadcrumb.css' %}   	            
        <link href="{{ asset_url }}" media="screen" rel="stylesheet" type="text/css" />
        {% endstylesheets %}


{% endblock %}

{% block body %}
	{{ parent() }}

        <script type='text/javascript'>
            
            var bluesgrass_blues_blues_viewstate_viewstate;
            
            {% autoescape false %}
                
                bluesgrass_blues_blues_viewstate_viewstate = new Bluegrass.Blues.Blues.ViewState.ViewState('{{ viewState.vars.requestParamName }}', '{{ viewState.vars.data }}');

            {% endautoescape %}
        </script>

{% endblock %}
